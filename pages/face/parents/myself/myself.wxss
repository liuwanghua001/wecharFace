page{
  background:#F2F2F2;
}
.myself{
  width:100%;
  height:100%;
  padding-bottom:200rpx;  
}
.myself-header-wrapper{
  width:100%;
  /* height:300rpx; */
  padding:30rpx 0;
  background:#FFFFFF;
  /* border:1px solid red;   */
}
.myself-header-wrapper .myself-header{
  width:92%;
  height:100%;
  /* background:pink; */
  margin:0 auto;
  display:flex;
}
.myself-header .myself-header-l{
  width:200rpx;
  /* background:red; */
}
.myself-header-l-head-wrapper{
  width:150rpx;
  height:150rpx;
  border: 1px solid rgba(169, 169, 169, 0.15);
  border-radius:10rpx;
}
.myself-header .myself-header-l .myself-header-l-head{
  /* width:200rpx;
  height:200rpx; */
  width:102rpx;
  height:124rpx;
  margin-top:10rpx;
  margin:12.5rpx auto 0 auto;
  display:block;
  /* border:1px solid red; */
}
.myself-header .myself-header-r{
  flex:1;
}
.myself-header .myself-header-r .myself-name{
  margin-top:10rpx;
  font-size:40rpx;  
  color:#000000;
}
.myself-header .myself-header-r .myself-school-class{
  color:#656565;
  font-size:30rpx;
  margin-top:20rpx;
}
.myself-header .myself-header-r .myself-school-class .class{
  margin-left:40rpx;
}
/* 块一 */
.black-one{
  width:92%;  
  background:#FFF;
  margin:31rpx auto;
  border-radius:14rpx;
}
.black-one .black-line{
  width:92%;  
  height:100rpx;
  line-height:100rpx;
  margin-left:8%;
  border-bottom:1px solid #F5F5F5;
  display:flex;
}
.black-one .black-line .print{
  width:50rpx;
}
.black-one .black-line .title{
  flex:1;
  color:#000000;
  font-size:30rpx;
  text-indent:20rpx;
}
.black-one .black-line .arr{
  width:80rpx;
  text-align:center;
}
.black-one .black-line .arr text{  
  transform:rotate(-90deg);
  display:block;
  color:#656565;  
}
.black-one-icon .black-line .print{
  font-size:50rpx;
}
/* 图标改色 */
.black-one-icon .black-line:nth-child(1) .print{
  color:#04C360;
}
.black-one-icon .black-line:nth-child(2) .print{
  color:#F45B6A;
  font-size:50rpx;
}

.black-two-icon .black-line:nth-child(1) .print{
  color:#4688F2;
}
.black-two-icon .black-line:nth-child(2) .print{
  color:#04C360;
  font-size:45rpx;
}
.black-two-icon .black-line:nth-child(3) .print{
  color:#F45B6A;
}
.black-two-icon .black-line:nth-child(4) .print{
  color:#4688F2;
  font-size:42rpx;
}
/* pages/face/model/model.wxss */
.shadow{
  width: 100%;
  height: 100%;
  z-index: 80;
  position: absolute;
  top: 0;
  background-color: #000;
  opacity: 0.5;
}
/* 上滑高度 */
.choosen{
  width: 100%;
  height: 35%; 
  position: fixed;
  bottom: 0;
  background-color: #fff;
  border-top-left-radius: 20rpx;
  border-top-right-radius: 20rpx;
  z-index: 98;
}
/* 内容 */
.container-box {
  display: flex;
  height: 100%;
  flex-direction: column;
  width: 100%;
  border-radius: 15rpx;
  margin: 0 auto;
  z-index: 98;
}
/* 按钮 */
.confirm-btn{
  display:flex;
  height:200rpx;
  text-align:center;
  margin-top: 30rpx;
}
.confirm-btn .confirm-btn-l{
  flex:1;  
}
.confirm-btn .confirm-btn-l .cancel,
.confirm-btn .confirm-btn-r .confirm{
  width:200rpx;
  height:80rpx;
  font-size:30rpx;  
  line-height:80rpx;
  border-radius:14rpx;
  margin:0 auto;
}
.confirm-btn .confirm-btn-l .cancel{  
  border:1px solid #656565;
  color:#656565;  
}
.confirm-btn .confirm-btn-r .confirm{
  background:#04C360;
  color:#fff;
}
.confirm-btn .confirm-btn-r{
  flex:1;
}
.container-box .title1{
  margin-top: 10rpx;
  margin-left: 30rpx;
  font-size: 40rpx;
}
.container-box .title2{
  width: 600rpx;
  font-size: 35rpx;
  margin: auto;
}
.limit .limit-quota{
  display:flex;
  width:92%;
  margin:0 auto;
  height:80rpx;
  line-height:100rpx;
}
.limit .limit-quota .limit-quota-l{
  width:160rpx;
  text-align:right;
  font-size:32rpx;
  color:#656565;
}
.limit .limit-quota .limit-quota-c{
  margin-top: 20rpx;
  height: 50rpx;
  border: 1px solid #ccc;
}
.limit .prompt-message{
  height:70rpx;
  text-align:center;
  color:#989898;
  line-height:70rpx;
  font-size:28rpx;
  padding-right:30rpx;
}
