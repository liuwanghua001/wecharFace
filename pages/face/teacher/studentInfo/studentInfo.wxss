  background:#fff;
}
.announcement-box .announcement {
  width:92%;
  height:900rpx;
}
.share-banner {
  display: flex;
  flex-flow: column wrap;
  align-items: flex-start;
  align-content: center;
  width:100%;
  /* border:1px solid pink; */
  /* background:red; */
  margin-left:25rpx;
}
