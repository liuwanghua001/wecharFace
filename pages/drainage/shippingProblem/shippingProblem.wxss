page,view{
  margin:0;
  padding:0;
}
.shippingProblem{
  width:100%;
  height:100%;
}	
.shippingProblem .shippingProblemListWrapper{
	width:90%;	
  height:auto;  
  margin:0 auto;	
}	
.shippingProblem .shippingProblemListWrapper .shippingProblemList{
  width:100%;
  background:#f7f8fa;
  padding:10rpx 0;
  box-shadow: 0px 2px 4px #eee;
  border-radius: 14rpx;
  margin:0 auto 40rpx auto;
}	
.shippingProblem .shippingProblemListWrapper .shippingProblemList .shippingProblemListTit{
  font-size:30rpx;
  font-weight:bold;
  padding:20rpx 0;
}	
.shippingProblem .shippingProblemListWrapper .shippingProblemList .shippingProblemListCon{
  font-size:28rpx;
  color:#666;
  line-height:50rpx;
}